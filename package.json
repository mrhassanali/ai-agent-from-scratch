{
  "name": "ai-agent-from-scratch",
  "version": "1.0.0",
  "description": "Agent from scratch",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prebuild": "npm-run-all clean && npm run lint",
    "build": "tsc",
    "dev": "nodemon --exec tsx src/index.ts",
    "start": "npx tsx src/index.ts",
    "watch": "tsc --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Hassan Ali",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "got": "^14.6.4",
    "lowdb": "^7.0.1",
    "openai": "^6.9.0",
    "ora": "^9.0.0",
    "prettier": "3.6.2",
    "terminal-image": "^4.1.0",
    "tsx": "^4.20.6",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  }
}
